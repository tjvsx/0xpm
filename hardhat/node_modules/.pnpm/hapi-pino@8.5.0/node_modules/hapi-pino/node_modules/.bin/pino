#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/pino@6.14.0/node_modules/pino/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/pino@6.14.0/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/hapi-pino@8.5.0/node_modules/hapi-pino/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/hapi-pino@8.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/pino@6.14.0/node_modules/pino/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/pino@6.14.0/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/hapi-pino@8.5.0/node_modules/hapi-pino/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/hapi-pino@8.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pino@6.14.0/node_modules/pino/bin.js" "$@"
else
  exec node  "$basedir/../../../../../pino@6.14.0/node_modules/pino/bin.js" "$@"
fi
