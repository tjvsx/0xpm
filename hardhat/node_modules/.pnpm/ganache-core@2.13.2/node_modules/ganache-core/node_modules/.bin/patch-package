#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/patch-package@6.2.2/node_modules/patch-package/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/patch-package@6.2.2/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/ganache-core@2.13.2/node_modules/ganache-core/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/ganache-core@2.13.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/patch-package@6.2.2/node_modules/patch-package/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/patch-package@6.2.2/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/ganache-core@2.13.2/node_modules/ganache-core/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/ganache-core@2.13.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../patch-package@6.2.2/node_modules/patch-package/index.js" "$@"
else
  exec node  "$basedir/../../../../../patch-package@6.2.2/node_modules/patch-package/index.js" "$@"
fi
