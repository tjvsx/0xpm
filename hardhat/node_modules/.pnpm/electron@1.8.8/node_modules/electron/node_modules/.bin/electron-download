#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules/electron-download/build/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules/electron-download/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron@1.8.8/node_modules/electron/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron@1.8.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules/electron-download/build/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules/electron-download/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron-download@3.3.0/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron@1.8.8/node_modules/electron/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/electron@1.8.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../electron-download@3.3.0/node_modules/electron-download/build/cli.js" "$@"
else
  exec node  "$basedir/../../../../../electron-download@3.3.0/node_modules/electron-download/build/cli.js" "$@"
fi
