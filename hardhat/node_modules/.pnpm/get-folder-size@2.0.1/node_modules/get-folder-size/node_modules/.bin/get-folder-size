#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules/get-folder-size/bin/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules/get-folder-size/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules/get-folder-size/bin/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules/get-folder-size/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/get-folder-size@2.0.1/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules/.pnpm/node_modules:/Users/thomasvanslooten/Projects/Atheneum/hardhat/node_modules:/Users/thomasvanslooten/Projects/Atheneum/node_modules:/Users/thomasvanslooten/Projects/node_modules:/Users/thomasvanslooten/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/get-folder-size" "$@"
else
  exec node  "$basedir/../../bin/get-folder-size" "$@"
fi
